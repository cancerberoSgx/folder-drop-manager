{
  "name": "folder-drop-manager",
  "version": "0.0.2",
  "description": "Easy API for Desktop Folder Drag&Drop to the browser",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc && tsc -p ./tsconfig-umd.json",
    "minify": "minify-fast --input dist-umd/**/*.js",
    "copy": "cp dist-umd/src/index.js docs/folder-drop-manager.js",
    "prepare": "rm -rf dist dist-umd && npm run build && npm run minify&& npm run copy"
  },
  "keywords": ["web file", "folder drag and drop", "html file api", "file api"],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "devDependencies": {
    "minify-fast": "^0.9.0",
    "typescript": "^2.9.2"
  }
}
